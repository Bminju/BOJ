// 첫째 줄에는 별 1개, 둘째 줄에는 별 2개, N번째 줄에는 별 N개를 찍는 문제

import java.util.Scanner;

public class Main{
	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		int n =scan.nextInt();
		int i , j; //i 행, j 별 갯수 
		for(i=1;i<=n;i++) {
			for(j=1; j<=i; j++) {
				System.out.printf("*");	
			}
			System.out.println(" ");
		}
	}
}
